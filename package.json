{
  "name": "mingling-app_client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --config ./shared/tooling/webpack.prod.js",
    "cy": "cypress open --config-file ./shared/config/cypress.json",
    "cy:features": "cypress run --config-file ./shared/config/cypress.json --spec \"**/*.features\"",
    "cz": "cz",
    "inspect:all": "npm run inspect:updates && npm run inspect:audit",
    "inspect:all:ci": "npm run inspect:updates:ci && npm run inspect:license && npm run inspect:audit",
    "inspect:audit": "audit-ci -h -c",
    "inspect:license": "license-checker --production --json --out ./licenses/licenses.json --failOn GPLv2",
    "inspect:updates": "ncu --doctor -u",
    "inspect:updates:ci": "ncu -e 2",
    "offline": "http-server dist",
    "pwa:assets": "pwa-asset-generator -i ./src/index.html -m ./src/manifest.webmanifest",
    "start": "webpack serve --progress --config ./shared/tooling/webpack.dev.js",
    "test": "jest --config ./shared/config/jest.json",
    "test:coverage": "jest --config ./shared/config/jest.json --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coryell1287/mingling-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coryell1287/mingling-app/issues"
  },
  "homepage": "https://github.com/coryell1287/mingling-app#readme",
  "browserslist": {
    "production": [
      ">= 0.5%",
      "not dead",
      "not op_mini all",
      "last 2 major versions",
      "Chrome >= 60",
      "Firefox >= 60",
      "not Edge < 79",
      "Firefox ESR",
      "iOS >= 10",
      "Safari >= 10",
      "Android >= 6",
      "not Explorer <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.{js,tsx,ts}": [
      "eslint --ignore-pattern \"/cypress/\" --fix \"*/**/*.{js,ts,tsx}\" --format table",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "commitizen": "^4.2.4",
